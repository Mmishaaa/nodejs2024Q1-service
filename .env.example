DATABASE_PORT=[your_databse_port]
DATABASE_DB=[your_databse_name]

DATABASE_URL=postgres://${DATABASE_USER}:${DATABASE_PASSWORD}@${DATABASE_HOST}:${DATABASE_PORT}/${DATABASE_DB}?schema=public

POSTGRES_USER=[docker_databse_user]
POSTGRES_PASSWORD=[docker_databse_password]
